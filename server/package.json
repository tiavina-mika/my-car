{
  "name": "my-car-server",
  "version": "1.0.0",
  "description": "MERN + REST stack test",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "npm run compile && node ./dist/index.js",
    "dev": "nodemon --exec babel-node ./index.js",
    "compile": "npm run clean && npm run build-babel",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel . --out-dir dist --ignore node_modules",
    "prepare:deploy": "cd .. && git subtree split --prefix server -b deploy && git add . && git commit -m 'prepare deploy' && git push -u origin deploy"
  },
  "author": "tiavina-mika",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-validation": "^3.0.8",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.10"
  },
  "devDependencies": {
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "nodemon": "^2.0.7"
  }
}
